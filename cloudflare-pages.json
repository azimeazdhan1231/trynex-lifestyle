
{
  "production_branch": "main",
  "build": {
    "command": "npm install && npx vite build",
    "destination": "dist/public",
    "root_dir": ".",
    "environment_variables": {
      "NODE_VERSION": "20"
    }
  },
  "functions": {
    "directory": "functions"
  },
  "compatibility_flags": [
    "nodejs_compat"
  ]
}
